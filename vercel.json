{
  "version": 2,
  "builds": [
    {
      "src": "web_app.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/web_app.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1",
    "UPLOAD_FOLDER": "/tmp/uploads",
    "RESULT_FOLDER": "/tmp/results"
  }
}